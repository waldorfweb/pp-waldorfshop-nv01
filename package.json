{
  "name": "Waldorfshop2",
  "license": "AGPL-3.0",
  "version": "5.0.33",
  "repository": {
    "type": "git",
    "url": "https://github.com/waldorfweb/pp-waldorfshop-02.git"
  },
  "scripts": {
    "start": "webpack --watch --progress",
    "build": "npm run build:prod && npm run build:dev",
    "build chunks": "npm i && npm run build",
    "build:dev": "webpack --progress",
    "build:prod": "webpack --env.prod --progress",
    "build:js": "npm run build:js:dev && npm run build:js:prod && npm run build:ssr",
    "build:js:dev": "webpack --config-name scripts --progress",
    "build:js:prod": "webpack --config-name scripts --env.prod --progress",
    "build:ssr": "npm run build:jsclient && npm run build:jsserver",
    "build:jsclient": "npm run build:jsclient:dev && npm run build:jsclient:prod",
    "build:jsclient:dev": "webpack --config-name client --progress",
    "build:jsclient:prod": "webpack --config-name client --env.prod --progress",
    "build:jsserver": "npm run build:jsserver:dev && npm run build:jsserver:prod",
    "build:jsserver:dev": "webpack --config-name server --progress",
    "build:jsserver:prod": "webpack --config-name server --env.prod --progress",
    "build:sass": "npm run build:sass:dev && npm run build:sass:prod",
    "build:sass:dev": "webpack --config-name styles --progress",
    "build:sass:prod": "webpack --config-name styles --env.prod --progress",
    "build:sass-vendor": "node tools/bundleSass.js",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:js": "eslint resources/js/src",
    "lint:sass": "stylelint \"resources/scss/**/*.scss\"",
    "prebuild": "npm run build:sass-vendor",
    "postbuild": "",
    "compareTranslations": "node tools/compareTranslations.js",
    "test": "npm run test:monetaryFormatter",
    "test:monetaryFormatter": "node -r esm tools/testMonetaryFormatter.js"
  },
  "browserslist": [
    "> .25%"
  ],
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "autoprefixer": "^8.6.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "cypress": "^6.2.0",
    "cypress-file-upload": "^4.1.1",
    "del": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.13.0",
    "eslint-loader": "^2.2.1",
    "esm": "^3.2.25",
    "expose-loader": "^0.7.5",
    "glob": "^7.1.6",
    "mini-css-extract-plugin": "^0.8.2",
    "moment-locales-webpack-plugin": "^1.2.0",
    "node-sass": "^4.14.1",
    "postcss": "^6.0.21",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^1.0.5",
    "q": "^1.5.1",
    "sass-loader": "^7.3.1",
    "serialize-javascript": "^3.1.0",
    "stylelint": "^12.0.1",
    "stylelint-config-twbs-bootstrap": "^1.1.0",
    "vue-loader": "^15.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-fix-style-only-entries": "^0.3.1",
    "webpack-require-from": "^1.8.1"
  },
  "dependencies": {
    "bootstrap": "4.4.1",
    "Ceres": "git+https://git@github.com/plentymarkets/plugin-ceres.git#5.0.33",
    "core-js": "^3.6.5",
    "custom-event-polyfill": "^0.3.0",
    "dayjs": "^1.8.26",
    "detect-browser": "^4.8.0",
    "flag-icon-css": "^2.9.0",
    "font-awesome": "^4.6.3",
    "jquery": "^3.5.1",
    "lodash": "^4.17.21",
    "owl.carousel": "2.2.0",
    "popper.js": "^1.16.1",
    "swiper": "^4.5.0",
    "vue": "^2.6.12",
    "vue-lazy-hydration": "^2.0.0-beta.4",
    "vue-template-compiler": "^2.6.12",
    "vuex": "^3.3.0"
  }
}
